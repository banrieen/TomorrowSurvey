   "source": [
    "period=100000\n",
    "n=$((6*1000000/$period))\n",
    "i=0\n",
    "{\n",
    "while [ $i -lt $n ]\n",
    "do\n",
    "d=$(date)\n",
    "echo $d 1>&2 \n",
    "top -n 1 |awk -v date=\"$d\" '/Xorg/{print $10\";\"$13\" \"date}'\n",
    "i=$(($i+1))\n",
    "usleep $period\n",
    "done \n",
    "} | tee >(awk -F\\; '{if($1>=max){max=$1;proname=$2}}END{print max\";\"proname}')"
   ]
